proc ModulesHelp {} {
        global version modroot

        puts stderr "vast - VANDLE Analysis Software Toolkit"
}
module-whatis "VAST provides software useful when analyzing VANDLE data."

set topdir      @CMAKE_INSTALL_PREFIX@

prepend-path PATH       $topdir/bin